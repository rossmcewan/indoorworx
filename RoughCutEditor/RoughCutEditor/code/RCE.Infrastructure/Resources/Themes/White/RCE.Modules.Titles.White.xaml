<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="TitlesBackgroundBrush" Color="Black"/>
    <LinearGradientBrush x:Key="TitleRectangleBackgroundBrush" StartPoint="0.421429,0.992423" EndPoint="0.421429,-9.24775e-007">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="#E50E4294" Offset="0"/>
            <GradientStop Color="#F25698C6" Offset="0.506977"/>
            <GradientStop Color="#FF9FEFF8" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="TitlePathFillBrush" Color="White"/>
    <Style x:Key="TitlesPathStyle" TargetType="Path">
    	<Setter Property="Fill" Value="{StaticResource TitlePathFillBrush}"/>
    	<Setter Property="Stroke" Value="Black"/>
    	<Setter Property="Effect">
    		<Setter.Value>
    			<DropShadowEffect Opacity="1" ShadowDepth="2.5"/>
    		</Setter.Value>
    	</Setter>
    	<Setter Property="StrokeThickness" Value="0.5"/>
    </Style>
</ResourceDictionary>
